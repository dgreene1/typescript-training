{
    "compilerOptions": {
      "allowJs": true,
      "module": "CommonJS",
      "target": "es6",
      "noImplicitAny": true,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "strictFunctionTypes": false,
      "strictNullChecks": true,
      "sourceMap": true,
      "baseUrl": "./",
      "outDir": "build",
      "lib": ["dom","es2016", "dom.iterable","scripthost", "es2015", "es2017"]
    },
    "compileOnSave": false,
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "**/*.compiled.*",
      "coverage",
      "node_modules",
      "typeScriptTempBuild",
      "testing-libs",
      "cypress",
      "./cypress",
      "../cypress",
      "cypress/**/*",
      "cypress/fixtures",
      "cypress/fixtures/**/*",
      "./cypress/fixtures/**/*",
      "cypress/**",
      "ts-node**"
    ],
    "atom": {
      "rewriteTsconfig": false
    }
  }
